<% if(success_msg!=""){ %> 
    <div class="safealert">
        <%= success_msg%>
      
        <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span> 
    </div>
  <% } %> 
  
  
  <% if(error_msg!=""){ %> 

    <div class="alert">
        <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span> 
        <%= error_msg%>
      </div>
  <% } %> 